            <url/>
            <status>0</status>
            <priority>3</priority>
            <description/>
            <type>0</type>
            <dependencies>
                <dependency>
                    <name>Memcache: Memory overload</name>
                    <expression>{MyTemplate_Memcache_agent:memcache[bytes].last(0)}&gt;({MyTemplate_Memcache_agent:memcache[limit_maxbytes].last(0)}*0.95)</expression>
                </dependency>
            </dependencies>
