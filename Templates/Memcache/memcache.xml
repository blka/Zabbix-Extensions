        </trigger>
        <trigger>
            <expression>{MyTemplate_Memcache_agent:net.tcp.service[tcp,,11211].last(0)}=0</expression>
            <name>Memcache: Is not available</name>
            <url/>
            <status>0</status>
            <priority>4</priority>
            <description/>
            <type>0</type>
            <dependencies>
                <dependency>
                    <name>Memcache: memcached is not running</name>
                    <expression>{MyTemplate_Memcache_agent:proc.num[memcached].last(0)}&lt;1</expression>
                </dependency>
            </dependencies>
        </trigger>
        <trigger>
            <expression>{MyTemplate_Memcache_agent:memcache[bytes].last(0)}&gt;({MyTemplate_Memcache_agent:memcache[limit_maxbytes].last(0)}*0.90)</expression>
